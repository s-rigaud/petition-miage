<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app
	xmlns="http://appengine.google.com/ns/1.0">

	<threadsafe>false</threadsafe>
	<sessions-enabled>true</sessions-enabled>
	<runtime>java8</runtime>

	<static-files>
		<include path="/**.html">
		</include>
		<include path="/**.js">
		</include>
	</static-files>
	
	<system-properties>
		<property name="java.util.logging.config.file"
			value="WEB-INF/logging.properties" />
	</system-properties>
	
	<env-variables>
		<env-var name="ENDPOINTS_SERVICE_NAME"
			value="petitions-31032021.appspot.com" />
	</env-variables>
</appengine-web-app>